QScrollBar:vertical {
	background: transparent;
	width: 10px;
    height: 100px;
	margin: 0px;
}
QScrollBar::handle:vertical {
	background-color: #e0e0e0;
}
QScrollBar::add-line:vertical, QScrollBar::sub-line:vertical {
	height: 0px;
	border: none;
}
QScrollBar::up-arrow:vertical, QScrollBar::down-arrow:vertical {
	height: 0px;
	width: 0px;
	border: none;
}